generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

model approved_links {
  entry_id     Int    @id @default(autoincrement())
  service_name String
  link         String @unique
}

model case_weights {
  service_id      Int     @id
  high_extreme    Int
  low_extreme     Int
  case_id         Int[]
  case_weight_sum Float[] @db.Real
}

model cases {
  case_id        Int    @id @default(autoincrement())
  title          String @unique
  classification Int
}

model new_links {
  entry_id     Int    @id @default(autoincrement())
  service_name String
  link         String @unique
}

model scored_links {
  link_id    Int    @id @default(autoincrement())
  link       String @unique
  service_id Int
}

model service_info {
  service_id   Int    @id @default(autoincrement())
  service_name String
  num_score    Float  @db.Real
  char_score   String
  case_ids     Int[]
}

model users {
  user_id       Int    @id @default(autoincrement())
  email         String @unique
  password_hash String
  case_scores   Json   @db.Json
}
