generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

model cases {
  id             Int    @id @default(autoincrement())
  title          String @unique @db.VarChar(255)
  classification String @db.VarChar(255)
}

model modified_services {
  entry_id   Int @id @default(autoincrement())
  service_id Int @unique
}

model new_links {
  entry_id     Int    @id @default(autoincrement())
  service_name String @db.VarChar(255)
  link         String @unique @db.VarChar(255)
}

model quotes {
  service_id Int
  link_id    Int
  quote_id   Int    @id @default(autoincrement())
  quote      String @db.VarChar(255)
}

model scored_links {
  service_id Int
  link_id    Int    @id @default(autoincrement())
  link       String @unique @db.VarChar(255)
}

model service_main {
  entry_id   BigInt  @id @default(autoincrement())
  service_id Int
  link_id    Int
  quote_id   Int
  value1     Int
  value2     Decimal @db.Decimal
}

model service_scores {
