generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "sqlite"
  url      = "file:./frontend2.db"
}

model cases {
  id             Int     @unique
  title          String? @unique
  classification String
}

model modified_services {
  service_id Int @unique
}

model new_links {
  service_id   Int
  service_name String
  link         String @unique
}

model quotes {
  service_id Int
  link_id    Int
  quote_id   Int    @unique
  quote      String
}

model scored_links {
  service_id Int
  link_id    Int    @unique
  link       String @unique
}

/// The underlying table does not contain a valid unique identifier and can therefore currently not be handled by Prisma Client.
model service_main {
  service_id Int @unique
  link_id    Int
  quote_id   Int
  value1     Int
  value2     Float
}

/// The underlying table does not contain a valid unique identifier and can therefore currently not be handled by Prisma Client.
